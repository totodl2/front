{
  "users": {
    "create": {
      "method": "PUT",
      "path": "/users"
    },
    "login": {
      "method": "POST",
      "path": "/users/login"
    }
  },
  "tokens": {
    "renew": {
      "method": "POST",
      "path": "/tokens/renew"
    }
  },
  "torrents": {
    "getAll": {
      "method": "GET",
      "path": "/torrents"
    },
    "get": {
      "method": "GET",
      "path": "/torrents/{hash}"
    },
    "pause": {
      "method": "POST",
      "path": "/torrents/{hash}/pause"
    },
    "start": {
      "method": "POST",
      "path": "/torrents/{hash}/start"
    },
    "remove": {
      "method": "DELETE",
      "path": "/torrents/{hash}"
    }
  }
}
